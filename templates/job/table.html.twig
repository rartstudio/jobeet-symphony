<table>
    <thead>
        <tr>
            <th>City</th>
            <th>Position</th>
            <th>Company</th>
        </tr>
    </thead>
    <tbody>
        {% for job in jobs %}
            <tr>
                <td>
                    {{ job.location }}
                </td>
                <td>
                    <a href="{{path('job.show', {id: job.id})}}">
                        {{ job.position }}
                    </a>
                </td>
                <td>
                    {{ job.company }}
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>